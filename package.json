{
  "name": "dormitory-booking-system",
  "version": "1.0.0",
  "description": "ระบบจองหอพักนักศึกษา",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "cd backend && npm run dev",
