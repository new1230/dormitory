    "dormitory",
    "booking",
    "react",
    "nodejs"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "dependencies": {
    "mysql2": "^3.14.1",
    "sequelize": "^6.37.7"
  }
}
